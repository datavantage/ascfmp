<?xml version="1.0"?>
<fmxmlsnippet type="FMObjectList">
	<Script includeInMenu="True" SiriShortcutVisible="False" runFullAccess="False" id="159" name="Loop Basic">
		<Step enable="True" id="141" name="Set Variable">
			<Value>
				<Calculation><![CDATA[0]]></Calculation>
			</Value>
			<Repetition>
				<Calculation><![CDATA[1]]></Calculation>
			</Repetition>
			<Name>$counter</Name>
		</Step>
		<Step enable="True" id="71" name="Loop">
			<FlushType value="Always"/>
		</Step>
		<Step enable="True" id="141" name="Set Variable">
			<Value>
				<Calculation><![CDATA[$counter+1]]></Calculation>
			</Value>
			<Repetition>
				<Calculation><![CDATA[1]]></Calculation>
			</Repetition>
			<Name>$counter</Name>
		</Step>
		<Step enable="True" id="89" name="# (comment)">
			<Text>Do something</Text>
		</Step>
		<Step enable="True" id="72" name="Exit Loop If">
			<Calculation><![CDATA[$counter > 10]]></Calculation>
		</Step>
		<Step enable="True" id="73" name="End Loop"/>
	</Script>
</fmxmlsnippet>
